$('.w-wisdoms').append('<%= escape_javascript(render partial: "wisdoms", :locals => { :wisdoms => @wisdoms })%>');
$('a.w-load-more-wisdoms').attr('href', '<%=wisdoms_path page: @wisdoms.next_page%>');
FB.XFBML.parse($('.w-wisdoms').get(0));
<% unless @wisdoms.next_page %>
  $('a.w-load-more-wisdoms').remove();
<% end %>

